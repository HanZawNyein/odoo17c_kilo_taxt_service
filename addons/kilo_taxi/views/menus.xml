<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- This Menu Item will appear in the Upper bar, That's why It needs NO parent or action -->
    <menuitem id="kilo_taxi_root" name="Kilo Taxi" sequence="0">
        <!-- This Menu Item Must have a parent -->
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="kilo_booking_categ" name="Booking"  action="kilo_taxi.kilo_booking_action" sequence="0"/>
        <menuitem id="configuration_categ" name="Configuration" sequence="1">
            <!-- This Menu Item must have a parent and an action -->
            <menuitem id="fleet_categ" name="Fleet" action="fleet.fleet_vehicle_action" sequence="0"/>
            <menuitem id="account_invoice_categ" name="Invoices" action="account.action_move_out_invoice_type" sequence="1"/>
            <menuitem id="res_config_settings_categ" name="Settings" action="kilo_taxi.kilo_taxi_res_config_setting_action" sequence="2"/>
        </menuitem>
    </menuitem>
</odoo>